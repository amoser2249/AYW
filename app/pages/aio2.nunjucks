            <div class="flex-wrapper">
              <div class="title">
                  <h1>Save up to $1,500 a year</h1>
                  <h4>Please verify your information</h4>
              </div>
              <div class="questions">
                <!-- Start Contact Info -->
                <fieldset data-name="ContactInfo">
                    <div class="field control-group first">
                        <label class="form__label" for="first-name">First Name</label>
                        <input class="form__input" data-name="FirstName" id="first-name" type="text" placeholder="First Name">
                    </div>
                </fieldset>
                <fieldset data-name="ContactInfo">
                    <div class="field control-group">
                        <label class="form__label" for="last-name">Last Name</label>
                        <input class="form__input" data-name="LastName" id="last-name" type="text" placeholder="Last Name">
                    </div>
                </fieldset>
                <fieldset data-name="ContactInfo">
                    <div class="field control-group">
                        <label class="form__label" for="email">Email</label>
                        <input class="form__input" data-name="Email" id="email" type="email" placeholder="youraddress@email.com">
                    </div>
                </fieldset>
                <fieldset data-name="ContactInfo">
                    <div class="field control-group">
                        <label class="form__label" for="phone">Phone</label>
                        <input class="form__input" data-name="Phone" id="phone" type="tel" placeholder="123-456-7890">
                    </div>
                </fieldset>
                <!-- End Contact Info -->

                <!-- Start Hidden Fields -->
                <fieldset data-name="ApplicantInfo">
                    <fieldset data-name="Credit">
                        <input type="hidden" data-name="SelfRating" value="Average">
                        <input type="hidden" data-name="Repossessions" value="false">
