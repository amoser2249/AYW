  <main>

<div class="wrap">
    <form id="solar-form" action="javascript:window.location = window.location+'&tqf=true';" class="main-form">
        <fieldset data-transition-type="fade" class="section active" data-page=0 data-page-title="/solar/1">
          <div class="flex-wrapper">
            <div class="title">
                <h1>Solar rebates available!</h1>
                <h4>Find out if you qualify</h4>
            </div>
            <div class="questions">
                <fieldset data-name="ApplicantInfo">
                    <fieldset data-name="Residence">
                        <div class="field control-group">
                            <label class="radio-label">Do you own a home?</label>
                            <div class="radio-control">
                                <div>
                                    <label for="Own">
                                        <input type="radio" id="Own" data-name="ResidenceStatus" name="RS" value="Own" data-autoadvance-click="true" checked>
                                        <span>Yes</span>
                                    </label>
                                </div>
                                <div>
                                    <label for="Rent">
                                        <input type="radio" id="Rent" data-name="ResidenceStatus" name="RS" data-autoadvance-click="true" value="Rent">
                                        <span>No</span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                    <button class="greenBtn" data-form-action-type="next">Start &raquo;</button>
                </fieldset>
            </div>
          </div>
        </fieldset>
        <!-- Begin Calculate Savings -->
        <fieldset data-transition-type="fade" class="section ready" data-page=1 data-page-title="/solar/2">
          <div class="flex-wrapper">
            <div class="title">
                <h1>Please enter the property information</h1>
            </div>
            <div class="questions">
              <fieldset class="solarAddress" data-name="ContactInfo" class="control-group">
