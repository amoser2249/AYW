                                      <path d="M0-.75h24v24H0z" fill="none" />
                                  </svg>
                              </div>
                          </div>
                          <input type="hidden" data-name="PropertyType" value="Single Family Home" />
                          <input type="hidden" data-name="RoofDesign" value="Gable" />
                          <input type="hidden" data-name="RoofType" value="Asphalt Shingle" />
                          <input type="hidden" data-name="Id" value="1" />
                          <input type="hidden" data-name="Address" />
                          <input type="hidden" data-name="City" />
                          <input type="hidden" data-name="State" />
                          <input type="hidden" data-name="ZipCode" />
                      </fieldset>
                  </fieldset>
                  <div class="pagenav onePageHide">
                      <button class="form__btn" data-form-action-type="next">Continue &raquo;</button>
                      <button class="btn-link back secondary" data-form-action-type="back">« Go Back</button>
                  </div>
              </fieldset>
            </div>
          </div>
        </fieldset>
        <!-- End Calculate Savings -->

        <!-- Start Contact Info -->
