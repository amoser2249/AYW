{% extends "layout.nunjucks" %}
{% block content %}
	<h1>This is the contact us page.</h1>
	{% import "macros/nav-macro.nunjucks" as nav %}
	{{ nav.active('contact')}}

	<div class="gallery">
		{% for image in images %}
			<div class="gallery__item">
				<img src="{{image.src}}" alt="{{image.alt}}">
			</div>
		{% endfor %}
	</div>
	<div class="testing"></div>
	<div class="github"></div>
{% endblock %}